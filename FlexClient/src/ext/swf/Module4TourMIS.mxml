<?xml version="1.0" encoding="utf-8"?>
<s:Module creationComplete="module1_creationCompleteHandler(event)" xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark" xmlns:mx="library://ns.adobe.com/flex/mx" width="0" height="0">
	<fx:Script>
		<![CDATA[
			import mx.events.FlexEvent;

			protected function module1_creationCompleteHandler(event:FlexEvent):void
			{
				ExternalInterface.addCallback("Fun4TourMIS_SelectDateRange", funSelectDateRange)
				ExternalInterface.addCallback("Fun4TourMIS_ArrangeSeatInfo", funArrangeSeatInfo)
				ExternalInterface.addCallback("Fun4TourMIS_SelectRegion", funSelectRegion)
			}

			private function funSelectDateRange(startDate:String, multiSelectAble:Boolean, iframeId:String, callFunname:String):void
			{
				WinSelectedDate.execute(startDate, multiSelectAble, iframeId, callFunname);
			}

			private function funArrangeSeatInfo(planitemId:String, orderId:String):void
			{
				WinArrangSeatInfo.execute(planitemId, orderId, 0);
			}

			private function funSelectRegion(iframeId:String, callFunname:String):void
			{
				WinRegion.execute(iframeId, callFunname);
			}
		]]>
	</fx:Script>
</s:Module>
