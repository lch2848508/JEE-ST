<?xml version="1.0" encoding="UTF-8"?>
<ehcache Name="PageCacheManager" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="false" monitoring="autodetect" dynamicConfig="true">
	<diskStore path="java.io.tmpdir" />
	
	<cache name="SimplePageCachingFilter" maxEntriesLocalHeap="10000" maxEntriesLocalDisk="1000" 
		eternal="false" overflowToDisk="true" timeToIdleSeconds="1296000" timeToLiveSeconds="1296000"
		memoryStoreEvictionPolicy="LFU" />
		
</ehcache>
